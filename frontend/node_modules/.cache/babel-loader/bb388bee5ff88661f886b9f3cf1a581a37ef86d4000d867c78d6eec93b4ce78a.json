{"ast":null,"code":"import axios from 'axios';\nimport styles from '@/assets/css/LoginView.module.css';\nexport default {\n  data() {\n    return {\n      email: '',\n      password: '',\n      styles\n    };\n  },\n  methods: {\n    async handleLogin() {\n      try {\n        const response = await axios.post('http://localhost:5000/api/auth/login', {\n          email: this.email,\n          senha: this.password\n        });\n        const token = response.data.access_token;\n        localStorage.setItem('token', token);\n        this.$router.push('/painel'); // redireciona para Painel\n      } catch (error) {\n        console.error('Erro no login:', error);\n        alert(error.response?.data?.message || 'Login inválido. Verifique o email e senha.');\n      }\n    }\n  }\n};","map":{"version":3,"names":["axios","styles","data","email","password","methods","handleLogin","response","post","senha","token","access_token","localStorage","setItem","$router","push","error","console","alert","message"],"sources":["E:\\PROGRAMAS\\agenda-facil\\frontend\\src\\views\\LoginView.vue"],"sourcesContent":["<template>\r\n  <div :class=\"styles['login-container']\">\r\n    <form :class=\"styles['login-form']\" @submit.prevent=\"handleLogin\">\r\n      <img src=\"../assets/logo.png\" alt=\"Logo\" :class=\"styles['login-logo']\" />\r\n\r\n      <input v-model=\"email\" type=\"email\" placeholder=\"Email\" :class=\"styles['login-input']\" required />\r\n      <input v-model=\"password\" type=\"password\" placeholder=\"Senha\" :class=\"styles['login-input']\" required />\r\n      <p :class=\"styles['login-forgot-password']\">Esqueci minha senha</p>\r\n      <button type=\"submit\" :class=\"styles['login-button']\">ENTRAR</button>\r\n      <p :class=\"styles['login-create-account']\">\r\n        Ainda não tem conta? <router-link to=\"/register\">Clique para criar uma conta</router-link>\r\n      </p>\r\n    </form>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios';\r\nimport styles from '@/assets/css/LoginView.module.css';\r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n      email: '',\r\n      password: '',\r\n      styles\r\n    };\r\n  },\r\n  methods: {\r\n    async handleLogin() {\r\n  try {\r\n    const response = await axios.post('http://localhost:5000/api/auth/login', {\r\n      email: this.email,\r\n      senha: this.password\r\n    });\r\n    const token = response.data.access_token;\r\n    localStorage.setItem('token', token);\r\n    this.$router.push('/painel'); // redireciona para Painel\r\n  } catch (error) {\r\n    console.error('Erro no login:', error);\r\n    alert(error.response?.data?.message || 'Login inválido. Verifique o email e senha.');\r\n  }\r\n}\r\n\r\n  }\r\n};\r\n</script>"],"mappings":"AAiBA,OAAOA,KAAI,MAAO,OAAO;AACzB,OAAOC,MAAK,MAAO,mCAAmC;AAEtD,eAAe;EACbC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,KAAK,EAAE,EAAE;MACTC,QAAQ,EAAE,EAAE;MACZH;IACF,CAAC;EACH,CAAC;EACDI,OAAO,EAAE;IACP,MAAMC,WAAWA,CAAA,EAAG;MACtB,IAAI;QACF,MAAMC,QAAO,GAAI,MAAMP,KAAK,CAACQ,IAAI,CAAC,sCAAsC,EAAE;UACxEL,KAAK,EAAE,IAAI,CAACA,KAAK;UACjBM,KAAK,EAAE,IAAI,CAACL;QACd,CAAC,CAAC;QACF,MAAMM,KAAI,GAAIH,QAAQ,CAACL,IAAI,CAACS,YAAY;QACxCC,YAAY,CAACC,OAAO,CAAC,OAAO,EAAEH,KAAK,CAAC;QACpC,IAAI,CAACI,OAAO,CAACC,IAAI,CAAC,SAAS,CAAC,EAAE;MAChC,EAAE,OAAOC,KAAK,EAAE;QACdC,OAAO,CAACD,KAAK,CAAC,gBAAgB,EAAEA,KAAK,CAAC;QACtCE,KAAK,CAACF,KAAK,CAACT,QAAQ,EAAEL,IAAI,EAAEiB,OAAM,IAAK,4CAA4C,CAAC;MACtF;IACF;EAEE;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}